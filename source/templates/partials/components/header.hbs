<header class="header">
  <nav class="navbar navbar-expand-lg navbar-light header__navbar">
    <div class="container">
      <a class="navbar-brand header__logo" href="#">
        <img src="{{data.img_url}}{{data.header.logo}}" alt="{{data.header.alt}}">
      </a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
        aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>

      <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav ml-auto header__menu">
          {{#each data.header.menuItems as |item indexItem|}}
          <li class="nav-item">
            <a class="nav-link" href="{{item.link}}">{{item.text}}</a>
          </li>
          {{/each}}
        </ul>
      </div>
    </div>
  </nav>
</header>