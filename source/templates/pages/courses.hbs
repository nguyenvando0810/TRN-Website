{{#> default}}
{{#*inline "content-body"}}
<section class="section courses">
   <div class="home__products box bg-light-grey">
    <div class="container">
      <div class="home__heading">
        {{contents.products.heading}}
      </div>
      <div class="row home__products-block">
        {{#each contents.products.items}}
          <div class="col-sm-6 col-lg-4 home__products-item" data-aos="fade-up">
            <div class="home__products-status">
              {{#is customer 'Developer'}}<p class="bg-red">{{customer}}</p>{{/is}}
              {{#is customer 'PM'}}<p class="bg-orange">{{customer}}</p>{{/is}}
              {{#is customer 'BA/Tester'}}<p class="bg-yellow">{{customer}}</p>{{/is}}
              {{#is customer 'Tester'}}<p class="bg-green">{{customer}}</p>{{/is}}
              {{#is customer 'Comtor'}}<p class="bg-blue">{{customer}}</p>{{/is}}
              {{#is customer 'Leadership Manager'}}<p class="bg-indigo">{{customer}}</p>{{/is}}
              {{#is customer 'Kh√°c'}}<p class="bg-purple">{{customer}}</p>{{/is}}
            </div>
            <div class="home__products-img-wrapper" style="background-image: url({{imgUrl}});">
            </div>
            <div class="home__products-content">
              <a class="home__products-title" href="{{title.href}}" target="{{title.target}}">{{title.text}}</a>
            </div>
          </div>
        {{/each}}
      </div>
    </div>
  </div>
</section>
{{/inline}}
{{/default}}